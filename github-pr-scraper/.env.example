# GitHub settings
GITHUB_BASE_URL=https://github.com
GITHUB_USE_EXISTING_SESSION=true
GITHUB_USER_DATA_DIR=  # Optional: path to browser profile directory

# Scraping settings
HEADLESS=false  # Set to false to see browser (useful for debugging)
SCRAPING_DELAY=1000
PAGE_TIMEOUT=30000
CONCURRENT_PAGES=3

# Output settings  
DEFAULT_OUTPUT_FORMAT=csv
DEFAULT_OUTPUT_DIR=./outputs

# Logging
LOG_LEVEL=INFO