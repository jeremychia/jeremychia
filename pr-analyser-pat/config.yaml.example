github:
  token: ${GITHUB_TOKEN}
  
organization: ${GITHUB_ORG}
user: ${GITHUB_USER}

analysis:
  date_range:
    from: null  # YYYY-MM-DD or null for all time
    to: null    # YYYY-MM-DD or null for all time
  
  filters:
    repositories: []  # Empty list means all repositories
    exclude_repositories: []
    branches: []  # Empty list means all branches
    pr_status: []  # open, closed, merged - empty means all

output:
  format: csv  # csv, json, markdown
  directory: ./reports
  visualizations: true
  
rate_limiting:
  requests_per_hour: 4000  # GitHub allows 5000/hour, keeping some buffer
  
logging:
  level: INFO  # DEBUG, INFO, WARNING, ERROR